{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutator": "javascript",
  "packageManager": "npm",
  "reporters": [
    "clear-text",
    "progress"
  ],
  "dashboard": {
    "baseUrl": "https://dashboard.stryker-mutator.io/api/reports",
    "project": "github.com/xRuiAlves/lichess-tournament-creator",
    "version": "master",
    "reportType": "full"
  },
  "testRunner": "jest",
  "maxConcurrentTestRunners": 4,
  "mutate": [
    "src/**/*.js",
    "!src/printer.js",
    "!src/main.js",
    "!src/lichessAPI.js"
  ],
  "transpilers": [],
  "coverageAnalysis": "off"
}
